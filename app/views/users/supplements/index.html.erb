<div class ="container">
	<div class = "row">
		<div class="col-xs-8 col-xs-offset-2">
			<h2 class="elementor-heading-title">SPULEMENT:</h2>
		</div>
	</div>
		<div class="row">
			<% @supplements.each do |supplement| %>
			<!-- <%# @supplement.each do |supplements| %> -->
	   			<div class="col-xs-2">
	   				<%#= link_to admins_supplement_path(supplement) do %>
						<div class="hoge">
							<button type="button" data-toggle="modal" data-target="#sampleModal-<%= supplement.id %> ">
								<div class="thumbnail">
									<%= attachment_image_tag supplement, :supple_image, :fill, 200, 200, format: 'jpeg' %>
									<div class="caption">
										<p class="text-over"><%= supplement.item_name.truncate(10) %></p>
										<p class="text-over"><%= supplement.description.truncate(10) %></p>
										<p class="text-over">￥<%= supplement.price.truncate(10) %></p>
										<p class="text-over"><%= supplement.maker.truncate(10) %></p>
										<p class="text-over"><%= supplement.genre.genre_name.truncate(10) %></p>

									</div>
								</div>
							</button>
						</div>
						<%= render 'show', { supplement: supplement } %>
						<div class="row admin-item likes_buttons_<%= supplement.id %>">
							<%= render partial: 'users/likes/like', locals: { supplement: supplement} %>
						</div>
							<!-- Supplement.allで取り出した１つをrenderでshowに引き渡す -->
					<%# end %>
	  			</div>
	  			<%# end %>
  			<% end %>
		</div>
</div>