	<div class ="container">
		<div class = "row">
			<div class="col-xs-8 col-xs-offset-2">
				<h2 class="elementor-heading-title">GYM SHOW:</h2>
			</div>
		</div>
			 <%= attachment_image_tag @gym, :gym_image,:fill, 60, 60, format: 'jpeg', class: "col-xs-2 col-xs-offset-1 pull-left profile-img", fallback: "no_image.jpg" %>

			<div class="col-xs-6">
				<div class="row admin-item">
					<h4 class="col-xs-3"></h4>
					<h4 class="col-xs-6"><%= @gym.gym_name %></h4>
				</div>

				<div class="row admin-item">
					<h4 class="col-xs-3"></h4>
					<h4 class="col-xs-6"><%= @gym.description %></h4>
				</div>

				<div class="row admin-item">
					<h4 class="col-xs-3"></h4>
					<h4 class="col-xs-6"><%= @gym.postal_code %></h4>
				</div>

				<div class="row admin-item">
					<h4 class="col-xs-3"></h4>
					<h4 class="col-xs-6"><%= @gym.address %></h4>
				</div>

				</div>
			</div>
		</div>
		<div class="row admin-item-show-btn">
			<%= link_to "編集する", edit_admins_gym_path(@gym),class: "col-xs-2 col-xs-offset-9 btn btn-primary" %>
			<%= link_to "一覧へ戻る", admins_gyms_path,class: "col-xs-2 col-xs-offset-9 btn btn-primary" %>
		</div>

	</div>